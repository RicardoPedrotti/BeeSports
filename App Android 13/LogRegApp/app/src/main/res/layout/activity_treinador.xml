        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/media_comparcao"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginBottom="244dp"
        android:layout_marginEnd="8dp"
