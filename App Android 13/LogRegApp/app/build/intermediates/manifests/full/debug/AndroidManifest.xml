        android:minSdkVersion="20"
        android:targetSdkVersion="20" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_logo1"
        android:label="BeeSports"
        android:roundIcon="@mipmap/ic_logo1"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Design.Light.NoActionBar" >
        <activity android:name="com.example.pedropifre.logregapp.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pedropifre.logregapp.DadosTreino" />
        <activity
            android:name="com.example.pedropifre.logregapp.treinador"
            android:label="@string/title_activity_treinador" />
        <activity
            android:name="com.example.pedropifre.logregapp.treino_inicial"
            android:label="@string/title_activity_treino_inicial" />
        <activity
            android:name="com.example.pedropifre.logregapp.registrar"
            android:label="registrar" />
        <activity
            android:name="com.example.pedropifre.logregapp.comecar_treino"
            android:label="@string/title_activity_comecar_treino" />
        <activity
            android:name="com.example.pedropifre.logregapp.escolha_jogador"
            android:label="@string/title_activity_escolha_jogador" />
        <activity
            android:name="com.example.pedropifre.logregapp.graficodelinha"
            android:label="@string/title_activity_line_chart" />
